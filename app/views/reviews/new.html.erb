<div class="container">
  <%= simple_form_for [ @restaurant, @review ] do |f| %>
    <%= f.input :content %>
    <%= f.input :rating %>
    <%= f.submit "Submit review", class: "btn btn-primary" %>
  <% end %>
  <br>
  <h4><%= link_to "Back to restaurant", restaurant_path(@restaurant)  %></h3>
</div>

